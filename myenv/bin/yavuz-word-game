#!/root/my_notebook_app/myenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'yavuz-unique==0.1.7','console_scripts','yavuz-word-game'
__requires__ = 'yavuz-unique==0.1.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('yavuz-unique==0.1.7', 'console_scripts', 'yavuz-word-game')()
    )
